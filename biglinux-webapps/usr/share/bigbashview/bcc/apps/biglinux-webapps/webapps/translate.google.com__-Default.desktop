#!/usr/bin/env xdg-open
[Desktop Entry]
Version=1.0
Terminal=false
Type=Application
MimeType=text/html;text/xml;application/xhtml_xml;
Name=Translator
Name[pt]=Tradutor
Exec=/usr/bin/biglinux-webapp --class="translate.google.com,Chromium-browser" --profile-directory=Default --app=https://translate.google.com
Icon=webapp-g-translate
StartupWMClass=translate.google.com
Categories=Google;
X-WebApp-URL=https://translate.google.com
